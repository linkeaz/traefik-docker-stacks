# Domain or subdomain used to access Netdata
NETDATA_DOMAIN=netdata.example.com

# BasicAuth hash generated with htpasswd
# Example: htpasswd -nbB admin "MY_SECURE_PASSWORD"
# Returns: admin:$2y$05$abc123...
NETDATA_USERHASH=admin:$2y$05$abc123...

# List of allowed IPs (e.g., 192.168.1.0/24, 1.2.3.4)
ALLOWED_IPS=192.168.1.0/24,1.2.3.4

# Timezone setting
TZ=Europe/Paris
